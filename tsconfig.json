{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "vite.config.ts",
    "typed-router.d.ts",
    "*.d.ts",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "components.d.ts",
    "src/firebase/firebase-messaging-sw.js",
    "firebase-messaging-sw.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "ignoreDeprecations": "5.0",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "vite-plugin-pwa/client",
      "firebase/firestore",
      "@types/webfontloader"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}